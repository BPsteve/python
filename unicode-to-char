    31545, 33384, 39770, 35399, 33089, 73838, 38516, 39022, 34370, 30273, 10
]

# Open the output file in write mode
with open('bktochar.txt', 'w', encoding='utf-8') as outfile:
    # Convert each code point to its corresponding character and write to file
    for code_point in code_points:
        try:
            char = chr(code_point)
            outfile.write(char)
        except ValueError:
            # Handle code points that are not valid characters
            outfile.write(f'[{code_point}]')

print("Decoding completed and saved to 'bktochar.txt'")
